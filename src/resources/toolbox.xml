<xml>
    <category name="Bukkit">
        <category name="基本">
            <block type="on_enable"></block>
            <block type="broadcast_0"></block>
            <block type="broadcast_1"></block>
            <block type="write_log"></block>
        </category>
        <category name="命令">
            <block type="on_command"></block>
        </category>
        <category name="事件">
            <block type="on_event">
                <field name="priority">normal</field>
                <value name="ignore_cancel">
                    <shadow type="logic_boolean">
                        <field name="BOOL">FALSE</field>
                    </shadow>
                </value>
            </block>
            <category name="玩家">
                <block type="event_player">
                    <field name="event">join</field>
                </block>
            </category>
        </category>
    </category>
</xml>